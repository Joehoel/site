{
	"name": "site",
	"version": "5.0.0",
	"scripts": {
		"dev": "astro dev",
		"start": "astro dev",
		"build": "astro build --remote",
		"postbuild": "pagefind --site dist",
		"preview": "astro preview",
		"lint": "biome lint .",
		"format": "bun run format:code && bun run format:imports",
		"format:code": "biome format . --write && prettier -w \"**/*\" \"!**/*.{md,mdx}\" --ignore-unknown --cache",
		"format:imports": "biome check --formatter-enabled=false --write",
		"check": "astro check"
	},
	"dependencies": {
		"@astrojs/db": "0.18.2",
		"@astrojs/markdoc": "0.15.8",
		"@astrojs/mdx": "4.3.9",
		"@astrojs/react": "4.4.1",
		"@astrojs/rss": "4.0.12",
		"@astrojs/sitemap": "3.6.0",
		"@astrojs/tailwind": "6.0.2",
		"@astrojs/vercel": "^9.0.0",
		"@fontsource-variable/albert-sans": "^5.2.5",
		"@keystatic/astro": "^5.0.6",
		"@keystatic/core": "^0.5.47",
		"@types/react": "^19.1.4",
		"@types/react-dom": "^19.1.5",
		"@vercel/analytics": "^1.5.0",
		"@vercel/speed-insights": "^1.2.0",
		"astro": "5.16.3",
		"astro-expressive-code": "^0.41.2",
		"astro-icon": "^1.1.5",
		"astro-robots-txt": "^1.0.0",
		"astro-webmanifest": "^1.0.0",
		"cssnano": "^7.0.6",
		"date-fns": "^4.1.0",
		"hastscript": "^9.0.1",
		"mdast-util-directive": "^3.1.0",
		"mdast-util-to-markdown": "^2.1.2",
		"mdast-util-to-string": "^4.0.0",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"rehype-external-links": "^3.0.0",
		"rehype-unwrap-images": "^1.0.0",
		"remark-directive": "^4.0.0",
		"satori": "0.12.1",
		"satori-html": "^0.3.2",
		"sharp": "^0.34.1",
		"unified": "^11.0.5",
		"unist-util-visit": "^5.0.0"
	},
	"devDependencies": {
		"@astrojs/check": "^0.9.4",
		"@astrojs/node": "^9.2.2",
		"@biomejs/biome": "^1.9.4",
		"@iconify-json/mdi": "^1.2.3",
		"@pagefind/default-ui": "^1.3.0",
		"@resvg/resvg-js": "^2.6.2",
		"@tailwindcss/typography": "^0.5.16",
		"@types/hast": "^3.0.4",
		"@types/mdast": "^4.0.4",
		"autoprefixer": "^10.4.20",
		"pagefind": "^1.3.0",
		"prettier": "^3.5.1",
		"prettier-plugin-astro": "0.14.1",
		"prettier-plugin-tailwindcss": "^0.7.1",
		"reading-time": "^1.5.0",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.7.3",
		"vite-tsconfig-paths": "^5.1.4"
	},
	"volta": {
		"node": "20.18.2",
		"pnpm": "10.4.1"
	},
	"packageManager": "pnpm@10.4.1",
	"pnpm": {
		"onlyBuiltDependencies": [
			"@biomejs/biome",
			"@vercel/speed-insights",
			"esbuild",
			"sharp"
		]
	}
}
